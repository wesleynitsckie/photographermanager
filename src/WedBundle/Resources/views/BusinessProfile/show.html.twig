{% extends 'WedBundle::base.html.twig' %}

{% block pageHeading %} Business Profile {% endblock %}

{% block content %}

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Facebookurl</th>
                <td>{{ entity.facebookUrl }}</td>
            </tr>
            <tr>
                <th>Twitterid</th>
                <td>{{ entity.twitterId }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ entity.user }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ entity.about }}</td>
            </tr>
            <tr>
                <th>Packages</th>
                <td>{{ entity.packages }}</td>
            </tr>
            <tr>
                <th>Photos</th>
                <td>{{ entity.photos }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('businessprofile') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('businessprofile_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
