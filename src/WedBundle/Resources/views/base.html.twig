<!DOCTYPE html>
<html>
<head>
    {% set user = app.security.getToken().getUser() %}
{% include 'WedBundle::head.html.twig' %}
    {% block css %}{% endblock %}
</head>
<body>
<body class="skin-blue">
<div class="wrapper">

    {%  include 'WedBundle::mainheader.html.twig' %}
    <!-- Left side column. contains the logo and sidebar -->
    {% include 'WedBundle::mainsidebar.html.twig' %}

    <!-- Content Wrapper. Contains page content -->


    <!-- /.content-wrapper -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                {% block pageHeading %} Page Heading {% endblock %}
                <small>{% block subheading %} Sub Heading {% endblock %}</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Dashboard</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            {% block content %}{% endblock %}
        </section>
    </div>
</div><!-- ./wrapper -->
{% include 'WedBundle::js.html.twig' %}
{% block js %}{% endblock %}
</body>
</html>
